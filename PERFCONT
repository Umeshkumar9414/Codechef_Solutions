#include <iostream>
using namespace std;
int main() {
	int t;
	scanf("%d",&t);
	while(t--)
	{
	
	int n,p,h=0,c=0,i;
	scanf("%d%d",&n,&p);
	int a[n];
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	
	for(i=0;i<n;i++)
	{
		if(a[i]<=p/10)
			h++;
			else if(a[i]>=p/2)
				c++;
				else
				{}
				
		
	}
			if(h==2 && c==1)
			printf("yes\n");
			else
			printf("no\n");
	}
	return 0;
} 
