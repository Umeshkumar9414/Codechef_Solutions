#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define db double 
#define z long long int 
using namespace std;
int main()
{
        
       z t ;
       sf("%lld",&t);
       while(t--)
       {
                z n ;
                sf("%lld",&n);
                pf("%lld\n",(n*(n-1))/2 );
                while(n--) {z a ; sf("%lld",&a);}
       }
 
} 
