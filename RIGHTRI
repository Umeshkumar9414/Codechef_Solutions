#include <bits/stdc++.h>
using namespace std;
int main() {
 
int f=0,x1,x2,x3,y1,y2,y3,t;
scanf("%d",&t);
while(t--)
{
scanf("%d %d %d %d %d %d",&x1,&y1,&x2,&y2,&x3,&y3);
 
if( (x3-x1)*(x2-x1)+(y3-y1)*(y2-y1)==0 || (x3-x2)*(x1-x2)+(y3-y2)*(y1-y2)==0 || (x1-x3)*(x2-x3)+(y1-y3)*(y2-y3)==0 )
{
	f++;
}
 
}
printf("%d",f);
	return 0;
 
	
} 
