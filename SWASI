#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define z long long int
using namespace std ;
int main()
{
	
	string s;
	z t,v,c;
	
	sf("%lld",&t);
 
	while(t--)
	{
			cin>>s;
				v=c=0;
			for(int i=0;i<s.length();i++)
			{
				if(s[i]=='a'|| s[i]=='e'||s[i]=='o'||s[i]=='i'||s[i]=='u')
					v++;
				else
					c++;
 
 
					
			}
 
			pf("%lld %lld\n",v,c);
	}
	return 0;
}  
